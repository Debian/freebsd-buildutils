
By default, install without -s.  Let dh_strip, etc handle stripping
(and honor DEB_BUILD_OPTIONS=nostrip, etc).

--- a/src/share/mk/bsd.lib.mk
+++ b/src/share/mk/bsd.lib.mk
@@ -40,7 +40,7 @@
 CTFFLAGS+= -g
 .endif
 .else
-STRIP?=	-s
+#STRIP?=	-s
 .endif
 
 .if ${MK_DEBUG_FILES} != "no" && empty(DEBUG_FLAGS:M-g) && \
